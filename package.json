{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm install --prefix con-web",
    "server": "nodemon ./bin/www",
    "client": "cd con-web && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "pretest": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "express": "~4.16.0",
    "faker": "^4.1.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jest": "^24.7.1",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "node-fetch": "^2.4.1"
  },
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "nodemon": "^1.18.10",
    "sinon": "^7.2.7"
  }
}
